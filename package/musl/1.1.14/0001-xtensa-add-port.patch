+	mov	a2, a4
+	mov	a4, a6
+	mov	a6, a2
+	mov	a5, a7
+	l32i	a8, a1, 16
+	movi	a2, 116 # SYS_clone
+	syscall
+
+	beqz	a2, 1f
+	// parent
+	retw
+
+	// child
+1:
+	movi	a0, 0
+	l32i	a4, a1, 0
+	l32i	a6, a1, 4
+	callx4	a4
